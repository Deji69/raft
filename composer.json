{
	"name": "raft/raft",
	"type": "library",
	"description": "Raft - really awesome file templates",
	"keywords": ["templating"],
	"license": "MIT",
	"authors": [
		{
			"name": "Deji",
			"email": "the_zone@hotmail.co.uk",
			"homepage": "https://github.com/Deji69",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": "^7.1"
	},
	"require-dev": {
		"mockery/mockery": "^1.2",
		"phpunit/phpunit": "^7.4",
		"psr/container": "^1.0"
	},
	"autoload": {
		"psr-4" : {
			"Raft\\" : "src/"
		}
	},
	"autoload-dev": {
		"psr-4" : {
			"Raft\\Tests\\" : "test/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.0.1-dev"
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
